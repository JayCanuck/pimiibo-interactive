{
  "name": "pimiibo-interactive",
  "version": "0.1.0",
  "description": "A tool to create your own amiibo with an interactive web frontend",
  "main": "server.js",
  "browser": "client",
  "contributors": [
    "Jason Robitaille",
    "Garrett Davidson"
  ],
  "license": "Apache-2.0",
  "private": true,
  "gypfile": true,
  "homepage": "https://github.com/JayCanuck/pimiibo-interactive#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JayCanuck/pimiibo-interactive.git"
  },
  "bugs": {
    "url": "https://github.com/JayCanuck/pimiibo-interactive/issues"
  },
  "scripts": {
    "start": "pm2 start server.js",
    "stop": "pm2 stop server.js",
    "startup": "pm2 startup",
    "save": "pm2 save",
    "unstartup": "pm2 unstartup",
    "pm2": "pm2",
    "serve": "enact serve",
    "pack": "enact pack",
    "pack-p": "enact pack -p",
    "watch": "enact pack --watch",
    "clean": "enact clean",
    "lint": "enact lint .",
    "license": "enact license",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "enact": {
    "theme": "agate"
  },
